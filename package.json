    "react-content-loader": "^6.2.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.2.2",
    "react-native": "0.66.5",
    "react-native-button-toggle-group": "^1.1.2",
    "react-native-default-preference": "^1.4.4",
    "react-native-dynamic": "^1.0.0",
